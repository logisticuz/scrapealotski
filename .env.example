DISCORD_BOT_TOKEN=

# Scraper settings
SCRAPE_CHANNEL_ID=
# Optional: comma-separated list of channels to process in one run.
SCRAPE_CHANNEL_IDS=
SCRAPE_LIMIT=0
SCRAPE_SINCE_DAYS=0
SCRAPE_USE_LAST_RUN=true
SCRAPE_STATE_PATH=scrape_state.json
# Optional base output folder for data/logs/state.
SCRAPE_OUTPUT_DIR=
# Optional separate media folder for images/videos.
SCRAPE_MEDIA_DIR=
SCRAPE_BACKFILL=false
SCRAPE_BATCH_SIZE=200
SCRAPE_BACKFILL_AUTORUN=false
SCRAPE_BACKFILL_SLEEP_SECONDS=10
SCRAPE_BACKFILL_MAX_BATCHES=0
SCRAPE_DRY_RUN=false
SCRAPE_METADATA_ONLY=false
SCRAPE_COUNT_ONLY=false
LOG_TO_FILE=false
LOG_PATH=scrape.log
LOG_ERRORS_TO_FILE=false
LOG_ERROR_PATH=scrape_errors.log
LOG_JSON=false
LOG_JSON_PATH=scrape_metrics.jsonl
DOWNLOAD_RETRIES=3
DOWNLOAD_BACKOFF_SECONDS=2
DOWNLOAD_TIMEOUT_SECONDS=30
MAX_ATTACHMENT_MB=0
# Dedupe index file to avoid re-downloading attachments.
DEDUPE_INDEX_PATH=scraped_data/dedupe_index.jsonl
IMAGE_EXTENSIONS=.png,.jpg,.jpeg,.gif
VIDEO_EXTENSIONS=.mp4,.mov,.webm,.mkv,.avi
DOCUMENT_EXTENSIONS=.txt,.pdf,.doc,.docx,.xls,.xlsx,.csv,.ppt,.pptx,.rtf,.odt,.ods,.odp

# Storage
ENABLE_UPLOADS=false
USE_DROPBOX=false
DROPBOX_ACCESS_TOKEN=
GCS_BUCKET_NAME=
GCS_CREDENTIALS_PATH=
# Cloud upload paths (only used when uploads enabled).
UPLOAD_BASE_PATH=/DiscordBot
UPLOAD_IMAGEBANK_PATH=/DiscordBot/ImageBank
